class Area {
    private int length;
    private int width;
    
    public void getArea(){
        int area = length * width;
        System.out.println("Area: " + area);
    }
    
    //no use
    public int getLength(){
        return length;
    }
    public int getWidth(){
        return width;
    }
    
    public void setLength (int newLength){
        length = newLength;
    }
    public void setWidth (int newWidth){
        width = newWidth;
    }
}

public class Main
{
    public static void main(String[] args) {
        Area a = new Area ();
        a.setLength(9);
        a.setWidth(9);
        a.getArea();
    }
}
