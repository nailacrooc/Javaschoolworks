public class CountChar {


    public static void main(String[] args) {

        String str = "www.oracle.com";

        int max = str.length();
        int count = 0;
        char quantE;
        
        for (int i = 0; i < max; i++){
            quantE = str.charAt(i);
            if (quantE == 'w'){
               count++; 
            }
        }
        System.out.println("Number of w : " + count );
    }
}
