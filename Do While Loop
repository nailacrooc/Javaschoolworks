import java.util.*;

public class Main
{
	public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        double b, c, d, e;
        boolean done;
  
      do {
      	//Remember that the user input must be inside the loop
        System.out.println("Enter Distance: ");
        float a = sc.nextFloat();
	
        if (a >= 10){
           b = a - 10;
           c = b * 2.5;
           System.out.print(c + 20 + " php");
           done = true;
        }else{
            System.out.println("Distance should be above 10km ");
            done = false;
        }
      }while (!done);
	
	}
}
